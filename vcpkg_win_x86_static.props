<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <VcpkgTriplet>x86-windows-static</VcpkgTriplet>
    <VcpkgEnabled>true</VcpkgEnabled>
    <!-- <VcpkgRoot>C:\libraries\vcpkg\installed\x86-windows-static\</VcpkgRoot> -->
  </PropertyGroup>
  <PropertyGroup>
    <!-- <_PropertySheetDisplayName>vcpkg_win_x86_static</_PropertySheetDisplayName> -->
  </PropertyGroup>
  <PropertyGroup Label="Globals" />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="VcpkgTriplet">
      <Value>$(VcpkgTriplet)</Value>
    </BuildMacro>
    <BuildMacro Include="VcpkgEnabled">
      <Value>$(VcpkgEnabled)</Value>
    </BuildMacro>
    <BuildMacro Include="VcpkgRoot">
      <Value>$(VcpkgRoot)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>